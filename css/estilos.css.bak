@charset "utf-8";
/* CSS Document */

html, body, div, span, h1, h2, h3, h4, h5, p, a, img, form, table, tr, td, fieldset, input, ul, li {
    border: 0 none;
    font-family: 'Roboto', sans-serif; 
    font-weight: normal;
    margin: 0;
    outline: 0 none;
    padding: 0;
}
.borrado {
    clear: both;
    font-size: 1px;
    line-height: 1px;
    margin: 0;
    padding: 0;
}


